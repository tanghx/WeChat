page{
    height: 100%;
}
.wifi-list{
    font-size: 12px;
    font-family: "微软雅黑";
    width: 100%;
}
.wifi-item{
    padding: 10rpx 20rpx;
    border-bottom: 1px solid #dedede;
    width: 100%;
    box-sizing: border-box;
}
.wifi-name{
    width: 100%;
    font-size: 14px;
    margin-bottom: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.wifi-address{
    color: #666;
    width: 100%;
    letter-spacing: 2rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.index{
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding-top: 200rpx; 
    background: #46c8ea;
}
.index-pic{
    display: block;
    margin: 0 auto;
}
.index-text{
    text-align: center;
    color: #fff;
    font-family: "微软雅黑";
    font-weight: bold;
}